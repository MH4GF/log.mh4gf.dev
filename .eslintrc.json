{
  "plugins": ["import-access", "jest"],
  "env": {
    "jest/globals": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:import/typescript",
    "plugin:storybook/recommended",
    "@mh4gf"
  ],
  "rules": {
    "import-access/jsdoc": ["error"],
    "@typescript-eslint/consistent-type-imports": ["error"]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
